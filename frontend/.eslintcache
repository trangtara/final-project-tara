oadingStatus = useSelector((store) => store.isLoading.isLoading)\n\n  const [name, setName] = useState('')\n  const [email, setEmail] = useState('')\n  const [password, setPassword] = useState('')\n\n  const [nameError, setNameError] = useState(null);\n  const [emailError, setEmailError] = useState(null);\n  const [passwordError, setPasswordError] = useState(null);\n  const [showValidations, setShowValidations] = useState(false);\n  const [formIsValid, setFormIsValid] = useState(false);\n\n  const nameIsValid = () => {\n    if (!name) {\n      setNameError('Name can not be blank');\n      return false;\n    } else if (name.length < 2) {\n      setNameError('Name should be longer than 2 letters');\n      return false;\n    } else if (name.length > 40) {\n      setNameError('Name should be less than 40 letters');\n      return false;\n    }\n    setNameError(null);\n    return true;\n  };\n\n  const passwordIsValid = () => {\n    if (!password) {\n      setPasswordError('Please type in your password');\n      return false;\n    } else if (password.length < 5) {\n      setPasswordError('Password needs to be longer than 5 characters');\n      return false;\n    }\n    setPasswordError(null);\n    return true;\n  };\n\n  const emailIsValid = () => {\n    if (!email) {\n      setEmailError('Email can not be blank');\n      return false;\n    } else if (!email.includes('@')) {\n      setEmailError('Email must follow email format');\n      return false;\n    }\n    setEmailError(null);\n    return true;\n  };\n\n  useEffect(() => {\n    const formIsValid = nameIsValid() && passwordIsValid() && emailIsValid()\n    setFormIsValid(formIsValid);\n  })\n\n  const handleSignup = (event) => {\n    event.preventDefault()\n    if (formIsValid) {\n      dispatch(signup(name, email, password));\n    } else {\n      setShowValidations(true);\n    }\n  }\n//Decide whether choosing the built-in validation or the customize validation error message\n  return (\n    <div>\n      <form\n      onSubmit={(event) => handleSignup(event)}>\n      <h3>Sign up new User</h3>\n        <div className=\"input-container\">\n          <div className=\"form-floating mb-3\">\n            <input\n            type=\"text\"\n            className=\"form-control\"\n            id=\"floatingInput\"\n            placeholder=\"username\"\n            value={name}\n            onChange={(event) => setName(event.target.value)}\n            />\n            <label \n            htmlFor=\"floatingInput\"\n            >User Name</label>\n          </div>\n          <div>\n            {showValidations && nameError}\n          </div>\n          <div className=\"form-floating mb-3\">\n            <input\n              required \n              type=\"email\"\n              className=\"form-control\"\n              id=\"floatingInput\"\n              placeholder=\"name@example.com\"\n              pattern=\"[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,}$\"\n              value={email}\n              onChange={(event) => setEmail(event.target.value)}\n            />\n            <label htmlFor=\"floatingInput\">User Email</label>\n          </div>\n          <div>\n            {showValidations && emailError}\n          </div>\n          <div className=\"form-floating mb-3\">\n            <input\n            className=\"form-control\"\n            type=\"password\"\n            id=\"floatingPassword\"\n            placeholder=\"Password\"\n            value={password}\n            onChange={(event) => setPassword(event.target.value)}\n            />\n            <label htmlFor=\"floatingPassword\">Password</label>\n          </div>\n          <div>\n            {showValidations && passwordError}\n          </div>\n        </div>\n        <button\n        className=\"btn btn-primary btn-sm\"\n        type= \"submit\">\n          Sign up\n          <span>\n            {loadingStatus &&\n            <Spinner animation=\"border\" size=\"sm\" role=\"status\" />}\n          </span>\n        </button>\n      </form>\n      <div className=\"other-option\">\n        <p>Already has an account?</p>\n        <Link \n          className=\"nav-link\"\n          to=\"/login\">\n            Log in\n        </Link>\n      </div>\n      {errorMessage && <p>{`${errorMessage}`}</p>}\n    </div>\n  )\n}\nexport default SignupForm\n","/Users/trangtara/Projects/final-project-tara/frontend/src/components/RegistrationForm.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/Navigation.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/reducers/loadingStatus.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/LoadingIndicator.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/pages/Attendants.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/AttendantList.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/pages/CheckinPage.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/Checkin.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/pages/NewEvent.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/common/Layout.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/NavigationItem.js",[],["100","101"],"/Users/trangtara/Projects/final-project-tara/frontend/src/reducers/attendants.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/common/Alert.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/AttendantListItem.js",[],{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","replacedBy":"105"},{"ruleId":"106","severity":1,"message":"107","line":67,"column":3,"nodeType":"108","endLine":67,"endColumn":12,"suggestions":"109"},{"ruleId":"102","replacedBy":"110"},{"ruleId":"104","replacedBy":"111"},"no-native-reassign",["112"],"no-negated-in-lhs",["113"],"react-hooks/exhaustive-deps","React Hook useEffect contains a call to 'setFormIsVali