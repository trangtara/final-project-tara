ct(() => {\n    const formIsValid = nameIsValid() && passwordIsValid() && emailIsValid();\n    setFormIsValid(formIsValid);\n  }, [name, password, email]);\n\n  const handleSignup = (event) => {\n    event.preventDefault()\n    if (formIsValid) {\n      dispatch(signup(name, email, password));\n    } else {\n      setShowValidations(true);\n    }\n  }\n//Decide whether choosing the built-in validation or the customize validation error message\n  return (\n    <div className=\"main-content\">\n      <h2 className=\"page-title\">Sign up new User</h2>\n      <form \n      className=\"form\"\n      onSubmit={(event) => handleSignup(event)}>\n        <div className=\"input-container\">\n          <label \n          className='label'\n          htlmlFor=\"name\">User Name</label>\n          <input\n          className=\"input\"\n          id=\"name\"\n          // required\n          type=\"text\"\n          minLength=\"2\"\n          value={name}\n          onChange={(event) => setName(event.target.value)}\n          />\n          <div>\n            {showValidations && nameError}\n          </div>\n          <label className=\"label\">Email</label>\n          <input\n          className=\"input\"\n          // required\n          // type=\"email\"\n          // placeholder=\"Email address\"\n          // pattern=\"[a-z0-9._%+-]+@[a-z0-9.-]+\\.[a-z]{2,}$\"\n          value={email}\n          onChange={(event) => setEmail(event.target.value)}\n          />\n          <div>\n            {showValidations && emailError}\n          </div>\n          <label className=\"label\">Password</label>\n          <input\n          className=\"input\"\n          // required\n          type=\"password\"\n          value={password}\n          onChange={(event) => setPassword(event.target.value)}\n          />\n          <div>\n            {showValidations && passwordError}\n          </div>\n        </div>\n        <button\n        className=\"button\"\n        type=\"submit\"\n        >\n          Sign Up\n        </button>\n      </form>\n      <div className=\"other-option\">\n        <p>Already has an account?</p>\n        <Link \n          className=\"nav-link\"\n          to=\"/login\">\n            Log in\n        </Link>\n      </div>\n      {errorMessage && <p>{`${errorMessage}`}</p>}\n    </div>\n  )\n}\nexport default SignupForm\n","/Users/trangtara/Projects/final-project-tara/frontend/src/components/RegistrationForm.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/QRcode.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/reducers/checkin.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/CheckinUpdate.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/pages/NavigationPage.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/components/Navigation.js",[],"/Users/trangtara/Projects/final-project-tara/frontend/src/pages/AttendantList.js",["79"],{"ruleId":"80","replacedBy":"81"},{"ruleId":"82","replacedBy":"83"},{"ruleId":"84","severity":1,"message":"85","line":67,"column":6,"nodeType":"86","endLine":67,"endColumn":29,"suggestions":"87"},{"ruleId":"84","severity":1,"message":"88","line":31,"column":6,"nodeType":"86","endLine":31,"endColumn":8,"suggestions":"89"},"no-native-reassign",["90"],"no-negated-in-lhs",["91"],"react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'emailIsValid', 'nameIsValid', and 'passwordIsValid'. Either include them or remove the dependency array.","ArrayExpression",["92"],"React Hook useEffect has a missing dependency: 'allAttendant_URL'. Either include it or remove the dependency array.",["93"],"no-global-assign","no-unsafe-negation",{"desc":"94","fix":"95"},{"desc":"96","fix":"97"},"Update the dependencies array to be: [name, password, email, nameIsValid, passwordIsValid, emailIsValid]",{"range":"98","text":"99"},"Update the dependencies array to be: [allAttendant_URL]",{"range":"100","text":"101